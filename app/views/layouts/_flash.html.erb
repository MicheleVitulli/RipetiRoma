<div class="row">
  <div class="col-6">
  <% flash.each do |key, msg| %>
    <div class="alert alert-<%=key%> alert-dismissible fade show" role="alert">
      <strong> <%= msg %> </strong>
      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close">
      </button>
    </div>
  <% end %>
</div>
</div >